    <script type="text/javascript">
      google.charts.load('current', {'packages':['corechart']});
      google.charts.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = google.visualization.arrayToDataTable([
          ['시간', '위험도'],
          ['3시간 전',  55],
          ['2시간 전',  67],
          ['1시간 전',  72],
          ['현재',  81],
          ['1시간 후',  84],
          ['2시간 후',  87],
          ['3시간 후',  91]
        ]);

        var options = {
          title: '현재 위험도 상황 & 예측',
          hAxis: {title: '시간',  titleTextStyle: {color: '#333'}},
          vAxis: {minValue: 0}
        };

        var chart = new google.visualization.AreaChart(document.getElementById('chart_div'));
        chart.draw(data, options);
      }
    </script>
    <script type='text/javascript' src='http://www.google.com/jsapi'></script>
<script type='text/javascript'>google.load('visualization', '1', {'packages': ['geochart']});
google.setOnLoadCallback(drawVisualization);

function drawVisualization() {
	var data = new google.visualization.DataTable();

	data.addColumn('string', 'Country');
	data.addColumn('number', 'Value'); 
	data.addColumn({type:'string', role:'tooltip'});var ivalue = new Array();
	
	data.addRows([[{v:'KR-11', f:'서울'},0,'위험도 82%']]);
	
	data.addRows([[{v:'KR-26', f:'부산'},1,'위험도 62%']]);
	
	data.addRows([[{v:'KR-27',f:'대구'},2,'위험도 42%']]);
	
	data.addRows([[{v:'KR-30',f:'대전'},3,'위험도 62%']]);
	
	data.addRows([[{v:'KR-29',f:'광주'},4,'위험도 81%']]);
	
	data.addRows([[{v:'KR-28',f:'인천'},5,'위험도 54%']]);
	
	data.addRows([[{v:'KR-31',f:'울산'},6,'위험도 35%']]);
	
	data.addRows([[{v:'KR-43',f:'충청북도'},7,'위험도 21%']]);
	
	data.addRows([[{v:'KR-44',f:'충청남도'},8,'위험도 15%']]);
	
	data.addRows([[{v:'KR-42',f:'강원도'},9,'위험도 75%']]);
	
	data.addRows([[{v:'KR-41',f:'경기도'},10,'위험도 52%']]);
	
	data.addRows([[{v:'KR-47',f:'경상북도'},11,'위험도 9%']]);
	
	data.addRows([[{v:'KR-48',f:'경상남도'},12,'위험도 23%']]);
	
	data.addRows([[{v:'KR-49',f:'제주도'},13,'위험도 12%']]);
	
	data.addRows([[{v:'KR-45',f:'전라북도'},14,'위험도 16%']]);
	
	data.addRows([[{v:'KR-46',f:'전라남도'},15,'위험도 45%']]);

	var options = {
		backgroundColor: {fill:'#FFFFFF',stroke:'#FFFFFF' ,strokeWidth:0 },
		colorAxis: {colors: ['#11c638', '#ef9708', '#e31b23']},
		legend: 'none',	
		backgroundColor: {fill:'#FFFFFF',stroke:'#FFFFFF' ,strokeWidth:0 },	
		datalessRegionColor: '#f5f5f5',
		displayMode: 'regions', 
		enableRegionInteractivity: 'true', 
		resolution: 'provinces',
		sizeAxis: {minValue: 1, maxValue:1,minSize:10,  maxSize: 10},
		region:'KR', //country code
		keepAspectRatio: true,
		width:600,
		height:400,
		tooltip: {textStyle: {color: '#444444'}, trigger:'focus'}	
	};
	
	var chart = new google.visualization.GeoChart(document.getElementById('visualization')); 
	google.visualization.events.addListener(chart, 'select');
	chart.draw(data, options);
}
 </script>
